{
  "name": "eventist-front-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.13",
    "@mui/material": "^5.15.13",
    "@mhayk/sagepay": "1.0.15",
    "@react-google-maps/api": "2.18.1",
    "@reduxjs/toolkit": "1.9.5",
    "axios": "1.4.0",
    "bootstrap": "^5.2.3",
    "cra-template-rb": "1.2.6",
    "crypto-browserify": "3.12.0",
    "css-loader": "^6.7.3",
    "exceljs": "4.3.0",
    "file-saver": "2.0.5",
    "formik": "2.4.1",
    "fs": "0.0.1-security",
    "https-browserify": "1.0.0",
    "jquery": "3.6.4",
    "js-cookie": "3.0.5",
    "jspdf": "2.5.1",
    "lightgallery": "2.7.1",
    "moment": "2.29.4",
    "opayo": "1.0.1",
    "primeicons": "^6.0.1",
    "primereact": "^9.2.3",
    "react": "^18.2.0",
    "react-awesome-lightbox": "^1.1.3",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-redux": "7.2.8",
    "react-router-dom": "6.8.1",
    "react-scripts": "5.0.1",
    "redux-saga": "1.2.3",
    "sass": "1.58.3",
    "swiper": "9.2.4",
    "xlsx": "0.18.5",
    "yup": "1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1"
  }
}
